{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key",
    "NEXT_PUBLIC_ONESIGNAL_APP_ID": "@next_public_onesignal_app_id"
  },
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "projectSettings": {
    "framework": "nextjs"
  }
}